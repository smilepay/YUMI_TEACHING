<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".stdMngBooking">


    <TextView
        android:id="@+id/textVD"
        android:layout_width="135dp"
        android:layout_height="42dp"
        android:text="예약 확인"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.094"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055"
        tools:ignore="MissingConstraints" />

    <Switch
        android:id="@+id/stdBookSwitch"
        android:layout_width="56dp"
        android:layout_height="28dp"
        android:layout_marginEnd="16dp"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.748"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.048"
        tools:ignore="RtlCompat" />
    <ListView
        android:id="@+id/std_booking_list"
        android:layout_width="360dp"
        android:layout_height="546dp"
        android:layout_margin="5dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="216dp"
        android:layout_weight="6"
        android:background="@drawable/draw1"
        android:padding="5dp"
        android:dividerHeight="0dp"
        android:divider="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVD"
        app:layout_constraintVertical_bias="0.12"
        android:layout_marginLeft="44dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="43dp"
        android:layout_height="24dp"
        android:text="미완료"
        app:layout_constraintBottom_toTopOf="@+id/std_booking_list"
        app:layout_constraintEnd_toStartOf="@+id/stdBookSwitch"
        app:layout_constraintHorizontal_bias="0.86"
        app:layout_constraintStart_toEndOf="@+id/textVD"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.55" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="34dp"
        android:layout_height="25dp"
        android:text="완료"
        app:layout_constraintBottom_toTopOf="@+id/std_booking_list"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.268"
        app:layout_constraintStart_toEndOf="@+id/stdBookSwitch"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.544" />

</androidx.constraintlayout.widget.ConstraintLayout>
